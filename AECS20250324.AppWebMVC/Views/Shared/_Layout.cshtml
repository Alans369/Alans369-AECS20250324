<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AECS20250324.AppWebMVC</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/AECS20250324.AppWebMVC.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top shadow-sm">
            <div class="container">
                <a class="navbar-brand font-weight-bold" asp-area="" asp-controller="Home" asp-action="Index">
                    <i class="fas fa-boxes mr-2"></i>AECS Inventory
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#mainNavbar"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="mainNavbar">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">
                                <i class="fas fa-home mr-1"></i> Home
                            </a>
                        </li>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Privacy">
                                <i class="fas fa-lock mr-1"></i> Privacy
                            </a>
                        </li>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white" asp-area="" asp-controller="Users" asp-action="Index">
                                <i class="fas fa-users mr-1"></i> Users
                            </a>
                        </li>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white" asp-area="" asp-controller="Brand" asp-action="Index">
                                <i class="fas fa-tags mr-1"></i> Brand
                            </a>
                        </li>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white" asp-area="" asp-controller="Warehouse" asp-action="Index">
                                <i class="fas fa-warehouse mr-1"></i> Warehouses
                            </a>
                        </li>
                        <li class="nav-item mx-1">
                            <a class="nav-link text-white" asp-area="" asp-controller="Product" asp-action="Index">
                                <i class="fas fa-box-open mr-1"></i> Product
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container mt-5 pt-4">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

  

    <!-- Bootstrap 4 JS y dependencias -->

    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/js/all.min.js"></script>
   

      <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.5/css/dataTables.bootstrap4.min.css">
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.js"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap4.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
